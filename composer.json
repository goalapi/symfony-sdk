{
  "name": "goalapi/symfony-sdk-bundle",
  "type": "symfony-bundle",
  "description": "Symfony SDK for GoalAPI football data service",
  "keywords": [
    "football",
    "soccer",
    "live score"
  ],
  "homepage": "https://github.com/goalapi/symfony-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Murat Erkenov",
      "email": "murat@11bits.net"
    },
    {
      "name": "11bits, Ltd.",
      "homepage": "http://11bits.net"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/framework-bundle": "^5.0",
    "guzzlehttp/guzzle": "^6.2",
    "symfony/serializer": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "GoalAPI\\SDKBundle\\": ""
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
